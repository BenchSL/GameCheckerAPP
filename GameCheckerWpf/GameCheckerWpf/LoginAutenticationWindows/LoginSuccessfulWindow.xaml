<Window x:Class="GameCheckerWpf.LoginAutenticationWindows.LoginSuccessfulWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameCheckerWpf.LoginAutenticationWindows"
        mc:Ignorable="d"
        Background="#181735"
        WindowStyle="None"
        ResizeMode="NoResize"
        MouseDown="Window_MouseDown"
        Title="LoginSuccessfulWindow" Height="450" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Button x:Name="btn_Proceed" FontFamily="Times New Roman" FontSize="30" Height="50" Width="300" Content="Proceed to main page" Grid.Row="2" Command="{Binding ProceedCommand}" CommandParameter="Home"></Button>
    </Grid>
</Window>
